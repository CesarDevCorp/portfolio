		"rss": "^1.2.2",
		"shiki": "^0.14.1",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-fragment-component": "^1.2.0",
		"svelte-media": "^0.1.5",
		"svelte-sitemap": "^2.6.0",
		"tailwindcss": "^3.2.6",
		"tslib": "^2.5.0",
		"typescript": "4.9.5",
		"vite": "^4.1.1",
		"vite-imagetools": "^4.0.18",
		"vitest": "^0.27.3"
	},
	"type": "module"
}
