{
	"name": "svelte-kit-tutorial",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test:e2e": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest -- --watch",
		"coverage": "vitest run --coverage",
		"test": "vitest run src -- --watch",
		"test:watch": "vitest src -- --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint . --ext .js,.ts,.svelte",
		"format": "prettier --plugin-search-dir . --write .",
		"fix": "npm run format && npm run lint -- --fix",
		"partytown": "partytown copylib static/~partytown",
		"prepare": "husky install",
		"postbuild": "svelte-sitemap --out-dir .vercel/output/static --domain https://cesar-mejia.com"
	},
	"devDependencies": {
		"@builder.io/partytown": "^0.7.5",
		"@lottiefiles/lottie-interactivity": "^1.6.2",
		"@playwright/test": "^1.30.0",
		"@prisma/client": "^4.10.1",
		"@skeletonlabs/skeleton": "^0.124.2",
		"@supabase/auth-helpers-sveltekit": "^0.8.7",
		"@supabase/supabase-js": "^2.7.1",
		"@sveltejs/adapter-vercel": "^1.0.6",
		"@sveltejs/kit": "^1.5.6",
		"@sveltejs/vite-plugin-svelte": "^2.0.2",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@tailwindcss/typography": "^0.5.9",
		"@testing-library/dom": "^8.20.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@testing-library/user-event": "^14.4.3",
		"@types/gtag.js": "^0.0.12",
		"@types/jest": "^29.4.0",
		"@types/rss": "^0.0.30",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"@vercel/analytics": "^0.1.8",
		"@vitest/coverage-c8": "^0.27.3",
		"autoprefixer": "^10.4.13",
		"clsx": "^1.2.1",
		"dayjs": "^1.11.7",
		"eslint": "^8.34.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-svelte3": "^4.0.0",
		"gray-matter": "^4.0.3",
		"husky": "^8.0.3",
		"iconify-icon": "^1.0.5",
		"jsdom": "^21.1.0",
		"lottie-web-light": "^1.1.0",
		"mdsvex": "^0.10.6",
		"party-js": "^2.2.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.2",
		"prisma": "^4.10.1",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-slug": "^5.1.0",
